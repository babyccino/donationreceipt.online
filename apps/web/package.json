{
  "name": "web",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:typesonly": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "email:dev": "email dev --port 3001 --dir ./src/emails",
    "stripe:forward": "stripe listen --forward-to localhost:3000/api/stripe/webhooks",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "bun test __tests__/unit/*",
    "test:integration": "NODE_ENV=test SEND_EMAILS_INTERNAL=true bun test __tests__/integration/* --preload ./__tests__/integration/setup.ts",
    "clean": "rm -rf ./.next"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.521.0",
    "@heroicons/react": "^2.0.18",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-email/components": "^0.0.12",
    "@react-email/render": "^0.0.7",
    "@react-pdf/renderer": "^3.1.14",
    "@sentry/nextjs": "^7.103.0",
    "better-sqlite3": "^9.2.0",
    "checksum": "^1.0.0",
    "components": "*",
    "db": "*",
    "downloadjs": "^1.4.7",
    "drizzle-orm": "^0.29.1",
    "file64": "^1.0.3",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.7.2",
    "jotai": "^2.6.4",
    "jszip": "^3.10.1",
    "lambdas": "*",
    "moment": "^2.30.1",
    "next": "^14.1.1",
    "next-auth": "^4.24.6",
    "next-seo": "^6.4.0",
    "nodemailer": "^6.9.10",
    "patch-package": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-pdf": "^7.7.1",
    "react-tailwindcss-datepicker": "^1.6.6",
    "sass": "^1.71.1",
    "sharp": "^0.33.2",
    "stripe": "^13.11.0",
    "tailwind-merge": "^1.14.0",
    "utils": "*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/checksum": "^0.1.35",
    "@types/downloadjs": "^1.4.6",
    "@types/node": "^20.10.3",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@types/use-sync-external-store": "^0.0.6",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.15",
    "eslint": "^8.55.0",
    "eslint-config-next": "^13.5.4",
    "msw": "^2.1.5",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.5",
    "types": "*",
    "typescript": "^5.3.2"
  },
  "browser": {
    "fs": false,
    "net": false,
    "child_process": false,
    "tls": false,
    "node:crypto": false,
    "request": false
  },
  "pnpm": {
