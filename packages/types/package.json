{
  "name": "types",
  "version": "1.0.0",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "files": ["dist/**"],
  "scripts": {
    "build:aws": "tsup src/* --format cjs",
    "build": "tsup src/* --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "dev": "tsup src/* --format esm,cjs --watch --dts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
